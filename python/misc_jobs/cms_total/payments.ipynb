    "import pandas as pd\n",
    "\n",
    "# Load the spreadsheet\n",
    "xl = pd.ExcelFile('Book1.xlsx')\n",
    "\n",
    "# Load a sheet into a DataFrame by its name\n",
    "df = xl.parse(xl.sheet_names[0])\n",
    "\n",
    "# Sum the first column\n",
    "sum_first_column = df.iloc[:, 0].sum()\n",
    "\n",
    "print(sum_first_column)"
   ]
  }
